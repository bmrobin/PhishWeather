{
  "name": "phish-weather",
  "version": "0.0.1",
  "description": "Weather service for Phish shows",
  "private": true,
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "build": "webpack --colors --progress --display-error-details --display-cached",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/jquery": "^2.0.40",
    "@types/node": "^7.0.5",
    "bootstrap": "3.3.7",
    "jasmine-core": "2.5.0",
    "jest": "^20.0.3",
    "jquery": "3.1.0",
    "jshint-stylish": "^2.1.0",
    "source-map-loader": "^0.1.6",
    "tslint": "^4.4.2",
    "ts-jest": "^20.0.3",
    "ts-loader": "^2.0.1",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/main/webapp/scripts/app/models/**",
      "src/main/webapp/scripts/app/services/**",
      "src/main/webapp/scripts/app/ui/**",
      "!src/dist/**"
    ],
    "mapCoverage": true,
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "engines": {
    "node": "^4.3"
  }
}
